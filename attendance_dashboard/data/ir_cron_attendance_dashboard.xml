<odoo>
    <record id="ir_cron_attendance_dashboard" model="ir.cron">
        <field name="name">Attendance: Missing Check-in Dashboard Refresh</field>
        <field name="model_id" ref="model_attendance_dashboard"/>
        <field name="state">code</field>
        <field name="code">model.refresh()</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
