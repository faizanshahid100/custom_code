<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_survey_survey_form_inherit" model="ir.ui.view">
        <field name="name">survey.survey.form.inherit.client.feedback</field>
        <field name="model">survey.survey</field>
        <field name="inherit_id" ref="survey.survey_survey_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position="before">
                <field name="is_client_feedback"/>
            </xpath>
            <xpath expr="//div[@name='button_box']" position="inside">
                <button name="action_export_excel" type="object" class="oe_stat_button" icon="fa-file-excel-o" string="Export Excel"/>
            </xpath>
        </field>
    </record>
</odoo>
